<script>
  function $(id){return document.getElementById(id)}
  var ul_lis = document.querySelectorAll("#ulink>li")
  var uls = document.querySelectorAll("#move>div")


       console.log($("ultwo"))


      for(var i=0;i<ul_lis.length;i++){
        ul_lis[i].index = i;
        ul_lis[i].onclick=function(){
            for(var j=0;j<uls.length;j++){
                uls[j].style.display = "none";
            }
         uls[this.index].style.display = "block";
        }
      }

      var ols=document.querySelectorAll("#olink>li")
      console.log(ols)



      for(var i=0;i<ols.length;i++){
        ols[i].index = i;
        ols[i].onclick = function(){
            $("ulone").style.left = -770*this.index+"px";
            $("ulone").style.transition =1 +"s";
            for(var j=0;j<ols.length;j++){
                ols[j].style.background = "#fff"
            }
            ols[this.index].style.background = "#ff0"
        }
      }

// 第二个录播图

   var lis =document.querySelectorAll("#like>li")
    console.log(lis)

    for(var i=0;i<lis.length;i++){
        lis[i].index = i;
        lis[i].onclick = function(){
            $("ultwo").style.left = -770*this.index+"px";
            $("ultwo").style.transition = 1+"s";
            for(var j=0;j<lis.length;j++){
                lis[j].style.background = "#fff"
            }
            lis[this.index].style.background = "#ff0"
        }
    }


</script>





